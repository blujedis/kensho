{
  "name": "@kensho/form",
  "version": "0.0.0.",
  "description": "Form state controllor with validation plugin support.",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "build:ts": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "build": "npm run clean && npm run build:ts",
    "dev": "tsc -b -w",
    "lint": "eslint *.ts*"
  },
  "author": {
    "name": "Blujedi LLC",
    "email": "blujedicorp@gmail.com"
  },
  "dependencies": {
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.set": "^4.3.2",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.has": "^4.5.7",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "17.0.41",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.0-dev.20220610"
  }
}