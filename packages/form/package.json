    "dev": "rollup --config ./rollup.config.js -w"
  },
  "author": {
    "name": "Blujedi LLC",
    "email": "blujedicorp@gmail.com"
  },
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2"
  },
  "devDependencies": {
    "@types/lodash.debounce": "^4.0.7",
    "@types/lodash.get": "^4.4.7",
    "@types/lodash.has": "^4.5.7",
    "@types/lodash.merge": "^4.6.7",
    "@types/lodash.set": "^4.3.7",
    "@types/node": "17.0.41",
    "eslint-config-root": "workspace:*",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.6",
    "rollup-config-default": "workspace:*",
    "tsconfig": "workspace:*",
    "typescript": "^4.8.0-dev.20220610"
  }
