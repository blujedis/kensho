{
  "name": "@kensho/adapter-svelte",
  "description": "Kensho Form store adapter for Svelte.",
  "version": "0.0.2",
  "cdn": "dist/index.umd.js",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "unpkg": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "jsdelivr": "dist/index.umd.js",
  "umd:main": "dist/index.umd.js",
  "exports": {
    ".": [
      {
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.js"
      },
      "./dist/index.js"
    ]
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "npm run clean && rollup --config ./rollup.config.js",
    "dev": "rollup --config ./rollup.config.js -w",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,md}'",
    "lint": "eslint src/*.ts*",
    "test": "echo \"Warn: no test specified in adapter-svelte\""
  },
  "author": {
    "name": "Blujedi LLC",
    "email": "blujedicorp@gmail.com"
  },
  "dependencies": {
    "@kensho/form": "workspace:*"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "@types/node": "17.0.41",
    "typescript": "^4.8.0-dev.20220610",
    "tsconfig": "workspace:*",
    "svelte": "^3.24.0",
    "rollup": "^2.75.6",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "eslint-config-prettier": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier-plugin-svelte": "^2.7.0"
  },
  "peerDependencies": {
    "svelte": "^0.0.1"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://blureg.blujedi.com"
  }
}